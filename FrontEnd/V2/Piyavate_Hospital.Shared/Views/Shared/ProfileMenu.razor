@rendermode InteractiveWebAssembly
<FluentProfileMenu Status="@PresenceStatus.Available" HeaderLabel="Piyavate-Hospital" Initials="BG" FullName="Bill Gates"
                   EMail="piyavate@piyavate.com" OnHeaderButtonClick="OnSignOut"
                   FooterLink="" />
                   @* Style="min-width: 330px;" *@
                   
@code{
    [Inject] CookieAuthenticationSateProvider AuthState { get; set; } = default!;

    async Task OnSignOut()
    {
        Console.WriteLine("Hello");
        await AuthState.LogOut();
        NavigationManager.NavigateTo("/");
    }

}
