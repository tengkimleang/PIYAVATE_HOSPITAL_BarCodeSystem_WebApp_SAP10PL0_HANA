@page "/GoodReturn/Mobile/Add/{DocEntry:int?}"
@using Piyavate_Hospital.Shared.Views.GoodReturn.MobileAppScreen.Add
@rendermode InteractiveWebAssembly
<PageTitle>Create Good Return</PageTitle>

<AuthorizeView Context="anotherName">
    <Authorized>
        <AddGoodReturnMobile DocEntry="DocEntry" Token="@anotherName.User.FindFirst("token")?.Value" />
    </Authorized>
</AuthorizeView>

@code{
    [Parameter] public int DocEntry { get; set; }
}